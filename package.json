{
  "name": "personal-portfolio-website",
  "version": "3.0.0",
  "description": "A personal portfolio site created for the repo owner (Aman Singh Bhogal.)",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --no-deprecation\" next dev",
    "devturbo": "pnpm dev --turbo",
    "devsafe": "rm -rf .next && pnpm dev",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "format": "npx prettier . --write",
    "lint-staged": "lint-staged",
    "test": "playwright test",
    "prepare": "husky",
    "payload": "payload",
    "generate:types": "payload generate:types",
    "generate:schema": "payload-graphql generate:schema",
    "standalone-script": "payload run ./src/scripts/standalone-script.ts",
    "generate:importmap": "payload generate:importmap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asbhogal/Personal-Portfolio-Site.git"
  },
  "keywords": [],
  "author": "Aman Singh Bhogal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asbhogal/Personal-Portfolio-Site/issues"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "homepage": "https://www.amansinghbhogal.com/",
  "dependencies": {
    "@axe-core/playwright": "^4.8.2",
    "@payloadcms/db-mongodb": "3.0.0-beta.80",
    "@payloadcms/db-postgres": "3.0.0-beta.80",
    "@payloadcms/next": "3.0.0-beta.80",
    "@payloadcms/richtext-lexical": "3.0.0-beta.80",
    "@payloadcms/richtext-slate": "3.0.0-beta.80",
    "@payloadcms/ui": "3.0.0-beta.80",
    "@splidejs/react-splide": "^0.7.12",
    "@vercel/analytics": "^0.1.8",
    "@vercel/speed-insights": "^1.0.1",
    "babel-plugin-react-compiler": "^0.0.0-experimental-592953e-20240517",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "framer-motion": "^9.0.3",
    "graphql": "^16.8.2",
    "mongodb": "^6.8.0",
    "next": "15.0.0-canary.104",
    "payload": "3.0.0-beta.80",
    "react": "19.0.0-rc-06d0b89e-20240801",
    "react-dom": "19.0.0-rc-06d0b89e-20240801",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.8.0",
    "react-websitecarbon-badge": "^1.0.5",
    "sharp": "0.32.6",
    "swr": "^2.2.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@innovixx/eslint-config": "^1.3.3",
    "@payloadcms/graphql": "3.0.0-beta.80",
    "@playwright/test": "^1.40.1",
    "@types/node": "^20.14.9",
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-sort-export-all": "^1.4.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "sass": "^1.57.1",
    "typescript": "^5.5.4"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.0",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
    }
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "bash -c tsc"
    ]
  }
}